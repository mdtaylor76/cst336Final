<div id="account">
    <% if (typeof logged != "undefined" && logged) { %>

        		<p id="navigation">[ <a class="<%= active === 'store' && 'active' %>" href="/store"> Store Search </a> ]
        		[ <a class="<%= active === 'cart' && 'active' %>" href="/cart"> Shopping Cart </a> ] </p>&nbsp;
        		<p id="logged">[ <%= username %>: <a class="button" href="/logout"><u>Logout</u></a> ]</p>

    <% } else { %>
        &nbsp;<p id="logged">Not Logged In</span>
    <% } %>
</div>
